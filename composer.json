{
  "name": "mynorel/framework",
  "description": "Mynorel: a narrative-first PHP framework with expressive architecture and directive syntax.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jade Monathrae Lewis",
      "email": "jadelewis@mynorel.dev",
      "role": "Architect & Founder"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "Mynorel\\": "src/Mynorel/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^12.3"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "php myne test || ./vendor/bin/phpunit"
  }
}
